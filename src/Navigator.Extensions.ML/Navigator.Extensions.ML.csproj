<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <Title>Navigator.Extensions.ML</Title>
        <Authors>Lucas Maximiliano Marino</Authors>
        <Description>Apply machine learning to your actions.</Description>
        <PackageProjectUrl>https://github.com/navigatorframework/navigator.extensions.ml</PackageProjectUrl>
        <RepositoryUrl>https://github.com/navigatorframework/navigator.extensions.ml</RepositoryUrl>
        <PackageIconUrl>https://raw.githubusercontent.com/navigatorframework/navigator/master/assets/logo.png</PackageIconUrl>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageTags>Machine, Learning, Bot, Framework, Navigator</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <PackageLicenseExpression>AGPL-3.0-only</PackageLicenseExpression>
        <Copyright>Copyright Â© Lucas Maximiliano Marino 2022</Copyright>
    </PropertyGroup>

    <ItemGroup>
        <None Include="../../README.md" Pack="true" PackagePath="/" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.ML" Version="2.0.0" />
        <PackageReference Include="Navigator" Version="1.9.3-beta1" />
    </ItemGroup>

</Project>
